<script src="/design/js/jquery.min.js"></script>
<script src="/design/bootstrap-3.3.7-dist/js/bootstrap.min.js"></script>
<script src="/design/js/plugins.js"></script>
<script src="/design/js/beetle.js"></script>
<script src="/design/js/slider.js"></script>
<script src="/scripts/jivosite.js"></script>
<script src="/scripts/Chart.bundle.min.js"></script>
<script>
    var config = {
        type: 'pie',
        data: {
            datasets: [{
                data: [<%- statistics.statistics.data %>],
                backgroundColor: [<%- statistics.statistics.colors %>],
                label: 'Usage by OS' // for legend
            }],
            labels: [<%- statistics.statistics.labels %>]
        },
        options: {
            responsive: true,
            legend: {
                position: 'top'
            },
            title: {
                display: false,
                text: 'Usage by OS'
            },
            animation: {
                animateRotate: true,
                animateScale: true
            }
        }
    };
    window.onload = function () {
        var ctx = document.getElementById('chart-area');
        window.myPolarArea = new Chart(ctx, config);
    };
    $('.count').each(function () {
        var $this = $(this);
        jQuery({Counter: 0}).animate({Counter: $this.text()}, {
            duration: 3000,
            easing: 'swing',
            step: function () {
                $this.text(Math.ceil(this.Counter));
            }
        });
    });
</script>
</body>
</html>
